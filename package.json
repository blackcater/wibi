{
  "name": "wibi",
  "version": "1.0.0",
  "description": "A beautiful, simple and clean theme.",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "./node_modules/.bin/gulp dev",
    "pro": "./node_modules/.bin/gulp pro",
    "lint": "./node_modules/.bin/gulp lint",
    "lint:test": "./node_modules/.bin/gulp lint:test",
    "clean": "./node_modules/.bin/gulp clean",
    "clean:test": "./node_modules/.bin/gulp clean:test",
    "test": "npm run lint:test && npm run clean:test && node_modules/.bin/jest --config jest.conf.json --coverage",
    "test:watch": "npm run lint:test && npm run clean:test && node_modules/.bin/jest --config jest.conf.json --coverage --watch",
    "e2e": "testcafe",
    "gh": "npm run pro && ./node_modules/.bin/gh-pages -d demo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blackcater/wibi.git"
  },
  "keywords": [
    "wibi",
    "css",
    "html",
    "responsive"
  ],
  "author": "blackcater",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blackcater/wibi/issues"
  },
  "homepage": "https://github.com/blackcater/wibi#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.1",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "browser-sync": "^2.18.8",
    "del": "^2.2.2",
    "eslint": "^3.17.1",
    "eslint-config-finger": "^0.2.0",
    "gh-pages": "^0.12.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-imagemin": "^3.2.0",
    "gulp-postcss": "^6.4.0",
    "gulp-pug": "^3.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-scp": "^0.0.3",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-stylus": "^2.6.0",
    "gulp-uglify": "^2.1.0",
    "jest": "^19.0.2",
    "testcafe": "^0.14.0"
  }
}
