{
  "name": "vsuite",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "repository": "https://github.com/vsuite/vsuite.git",
  "author": "blackcater <blackcater2015@gmail.com>",
  "workspaces": [
    "packages/*"
  ],
  "files": [
    "lib"
  ],
  "scripts": {
    "dev": "start-storybook -p 9001 -s ./public,./brand -c .storybook",
    "dist": "",
    "dist:style": "",
    "lint": "eslint --fix '{.storybook,src,docs,package}/**/*.{js,jsx,vue}' && stylelint --fix '{.storybook,src,docs,package}/**/*.less'",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-coverage": "jest --coverage",
    "prettier": "stylelint-config-prettier-check && prettier --write '{.storybook,src,docs,package}/**/*.{js,jsx,vue,less}'",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:deploy": "sh docs/deploy.sh"
  },
  "dependencies": {
    "@vsuite/schema-typed": "*",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "popper.js": "^1.14.4",
    "tinycolor2": "^1.4.1",
    "v-click-outside-x": "^3.3.0",
    "vue-types": "^1.3.3"
  },
  "peerDependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@commitlint/cli": "^7.2.0",
    "@commitlint/config-angular": "^7.1.2",
    "@storybook/addon-actions": "^4.0.0-alpha.24",
    "@storybook/addon-backgrounds": "^4.0.0-alpha.24",
    "@storybook/addon-links": "^4.0.0-alpha.24",
    "@storybook/addon-notes": "^4.0.0-alpha.24",
    "@storybook/addon-options": "^4.0.0-alpha.24",
    "@storybook/addon-storysource": "^4.0.0-alpha.24",
    "@storybook/addon-viewport": "^4.0.0-alpha.24",
    "@storybook/addons": "^4.0.0-alpha.24",
    "@storybook/vue": "^4.0.0-alpha.22",
    "@vue/server-test-utils": "^1.0.0-beta.25",
    "@vue/test-utils": "^1.0.0-beta.25",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
    "babel-plugin-transform-vue-jsx": "^4.0.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "file-loader": "^2.0.0",
    "husky": "^1.1.1",
    "jest": "^23.6.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "regenerator-runtime": "^0.12.1",
    "style-loader": "^0.23.1",
    "stylelint": "^9.6.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "svg-sprite-loader": "^4.1.2",
    "url-loader": "^1.1.1",
    "vue": "^2.5.17",
    "vue-eslint-parser": "^3.2.2",
    "vue-jest": "^2.6.0",
    "vue-json-tree-view": "^2.1.4",
    "vue-loader": "^15.4.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.17"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{.storybook,src,docs,package}/**/*.{js,jsx,vue}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "{.storybook,src,docs,package}/**/*.less": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/dist/",
      "/docs/",
      "/examples?/",
      "/node_modules/"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.(vue)$": "vue-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(@storybook/.*\\.vue$))"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "node"
    ],
    "bail": true
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 9"
  ],
  "license": "MIT",
  "private": true
}
