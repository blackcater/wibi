{
  "name": "vsuite",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "repository": "https://github.com/vsuite/vsuite.git",
  "author": "blackcater <blackcater2015@gmail.com>",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "start-storybook -p 9001 -s ./public,./brand -c .storybook",
    "dist": "",
    "dist:style": "",
    "lint": "eslint --fix .",
    "test": "yarn prettier && yarn lint && jest",
    "prettier": "prettier --write 'src/**/*.{js,vue}'"
  },
  "dependencies": {
    "async-validator": "^1.8.5",
    "deepmerge": "^2.1.1",
    "element-resize-detector": "^1.1.14",
    "js-calendar": "^1.2.3",
    "lodash.throttle": "^4.1.1",
    "popper.js": "^1.14.3",
    "tinycolor2": "^1.4.1",
    "v-click-outside-x": "^3.0.3"
  },
  "peerDependencies": {
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-angular": "^7.0.1",
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-backgrounds": "^3.4.8",
    "@storybook/addon-knobs": "^3.4.8",
    "@storybook/addon-links": "^3.4.8",
    "@storybook/addon-notes": "^3.4.8",
    "@storybook/addon-options": "^3.4.8",
    "@storybook/addon-storysource": "^3.4.8",
    "@storybook/addon-viewport": "^3.4.8",
    "@storybook/addons": "^3.4.8",
    "@storybook/vue": "^3.4.8",
    "@vue/test-utils": "^1.0.0-beta.21",
    "@vue/server-test-utils": "^1.0.0-beta.21",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-vue": "^4.7.1",
    "husky": "^1.0.0-rc.13",
    "jest": "^23.4.1",
    "jest-vue-preprocessor": "^1.4.0",
    "less": "^3.8.0",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "style-loader": "^0.21.0",
    "vue": "^2.5.16",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/dist/",
      "/docs/",
      "/examples?/",
      "/node_modules/"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(@storybook/.*\\.vue$))"
    ],
    "moduleFileExtensions": [
      "vue",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "bail": true
  },
  "license": "MIT",
  "private": true
}
